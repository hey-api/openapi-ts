{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "port": 9229,
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "openapi-ts",
      "skipFiles": ["<node_internals>/**"],
      "cwd": "${workspaceFolder}/dev",
      "runtimeExecutable": "node",
      "program": "${workspaceFolder}/packages/openapi-ts/dist/run.mjs",
      "args": [],
      "env": {
        "DEBUG": "false"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "openapi-python",
      "skipFiles": ["<node_internals>/**"],
      "cwd": "${workspaceFolder}/dev",
      "runtimeExecutable": "node",
      "program": "${workspaceFolder}/packages/openapi-python/dist/run.mjs",
      "args": [],
      "env": {
        "DEBUG": "false"
      }
    }
  ]
}
