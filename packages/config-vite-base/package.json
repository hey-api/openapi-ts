{
  "name": "@config/vite-base",
  "version": "0.0.1",
  "description": "Base configuration for Vite and Vitest",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsup"
  },
  "exports": {
    ".": {
