      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "dependencies": {
    "vite": "^6.2.7",
    "vitest": "^3.1.1"
  },
  "devDependencies": {
    "typescript": "^5.8.3"
  },
  "private": true
}
