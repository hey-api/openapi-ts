{
  "name": "@hey-api/openapi-ts",
  "version": "0.86.4",
  "description": "ðŸš€ The OpenAPI to TypeScript codegen. Generate clients, SDKs, validators, and more.",
  "homepage": "https://heyapi.dev/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hey-api/openapi-ts.git"
  },
  "bugs": {
    "url": "https://github.com/hey-api/openapi-ts/issues"
  },
  "license": "MIT",
  "author": {
    "email": "lubos@heyapi.dev",
    "name": "Hey API",
    "url": "https://heyapi.dev"
  },
  "funding": "https://github.com/sponsors/hey-api",
  "keywords": [
    "angular",
    "axios",
    "codegen",
    "fetch",
    "generator",
    "http",
    "javascript",
    "json",
    "next",
    "next.js",
    "node",
    "nuxt",
    "ofetch",
    "openapi",
    "rest",
    "swagger",
    "typescript",
    "xhr",
    "yaml"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./internal": {
      "import": {
        "types": "./dist/internal.d.ts",
        "default": "./dist/internal.js"
      },
      "require": {
        "types": "./dist/internal.d.cts",
        "default": "./dist/internal.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "openapi-ts": "bin/index.cjs"
  },
  "files": [
    "bin",
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown && pnpm check-exports",
    "check-exports": "attw --pack . --profile node16",
    "dev": "tsdown --watch",
    "handlebars": "node src/legacy/handlebars/handlebars.cjs",
    "prepublishOnly": "pnpm build",
    "test:coverage": "vitest run --coverage",
    "test:update": "vitest watch --update",
    "test:watch": "vitest watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "dependencies": {
    "@hey-api/codegen-core": "workspace:^0.3.1",
    "@hey-api/json-schema-ref-parser": "catalog:prod",
    "ansi-colors": "catalog:prod",
    "c12": "catalog:prod",
    "color-support": "catalog:prod",
    "commander": "catalog:prod",
    "handlebars": "catalog:prod",
    "open": "catalog:prod",
    "semver": "catalog:prod"
  },
  "peerDependencies": {
    "typescript": "catalog:peer"
  },
  "devDependencies": {
    "@angular/common": "catalog:dev",
    "@angular/compiler": "catalog:dev",
    "@angular/compiler-cli": "catalog:dev",
    "@angular/core": "catalog:dev",
    "@angular/platform-browser": "catalog:dev",
    "@angular/platform-browser-dynamic": "catalog:dev",
    "@angular/router": "catalog:dev",
    "@config/vite-base": "workspace:*",
    "@types/bun": "catalog:dev",
    "@types/cross-spawn": "catalog:dev",
    "@types/semver": "catalog:dev",
    "axios": "catalog:dev",
    "cross-spawn": "catalog:dev",
    "eslint": "catalog:dev",
    "glob": "catalog:dev",
    "nuxt": "catalog:dev",
    "ofetch": "catalog:dev",
    "prettier": "catalog:dev",
    "rxjs": "catalog:dev",
    "typescript": "catalog:dev",
    "vue": "catalog:dev",
    "yaml": "catalog:dev",
    "zone.js": "catalog:dev"
  }
}
