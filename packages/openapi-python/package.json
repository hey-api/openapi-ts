{
  "name": "@hey-api/openapi-python",
  "version": "0.0.2",
  "private": true,
  "description": "ðŸ OpenAPI to Python codegen.",
  "homepage": "https://heyapi.dev/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hey-api/openapi-ts.git"
  },
  "bugs": {
    "url": "https://github.com/hey-api/openapi-ts/issues"
  },
  "license": "MIT",
  "author": {
    "email": "lubos@heyapi.dev",
    "name": "Hey API",
    "url": "https://heyapi.dev"
  },
  "funding": "https://github.com/sponsors/hey-api",
  "keywords": [
    "codegen",
    "generator",
    "http",
    "javascript",
    "json",
    "node",
    "openapi",
    "python",
    "rest",
    "swagger",
    "typescript",
    "yaml"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "openapi-python": "./bin/run.js"
  },
  "files": [
    "bin",
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown && pnpm check-exports",
    "check-exports": "attw --pack . --profile node16",
    "dev": "tsdown --watch",
    "prepublishOnly": "pnpm build",
    "test:coverage": "vitest run --coverage",
    "test:update": "vitest watch --update",
    "test:watch": "vitest watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "dependencies": {
    "@hey-api/codegen-core": "workspace:^0.5.5",
    "@hey-api/json-schema-ref-parser": "1.2.2",
    "@hey-api/types": "workspace:*",
    "ansi-colors": "4.1.3",
    "color-support": "1.1.3",
    "commander": "14.0.2",
    "open": "11.0.0",
    "semver": "7.7.3"
  },
  "peerDependencies": {
    "typescript": ">=5.5.3"
  },
  "devDependencies": {
    "@config/vite-base": "workspace:*",
    "@types/cross-spawn": "6.0.6",
    "@types/semver": "7.7.1",
    "cross-spawn": "7.0.6",
    "eslint": "9.39.1",
    "prettier": "3.4.2",
    "typescript": "5.9.3",
    "yaml": "2.8.2"
  }
}
