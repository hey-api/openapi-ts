{
  "name": "@hey-api/client-nuxt",
  "version": "0.4.0",
  "description": "ðŸš€ Nuxt client for `@hey-api/openapi-ts` codegen.",
  "homepage": "https://heyapi.dev/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hey-api/openapi-ts.git"
  },
  "bugs": {
    "url": "https://github.com/hey-api/openapi-ts/issues"
  },
  "license": "MIT",
  "author": {
    "email": "lubos@heyapi.dev",
    "name": "Hey API",
    "url": "https://heyapi.dev"
  },
  "funding": "https://github.com/sponsors/hey-api",
  "keywords": [
    "client",
    "codegen",
    "fetch",
    "http",
    "javascript",
    "nuxt",
    "openapi",
    "rest",
    "swagger",
    "typescript",
    "vue"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "LICENSE.md",
    "src"
  ],
  "scripts": {
    "build": "tsup && rollup -c && pnpm check-exports",
    "check-exports": "attw --ignore-rules cjs-resolves-to-esm --pack .",
    "dev": "tsup --watch",
    "prepublishOnly": "pnpm build",
    "test:coverage": "vitest run --coverage",
    "test:update": "vitest watch --update",
    "test:watch": "vitest watch",
    "test": "vitest run",
    "typecheck": "vitest --typecheck --watch=false"
  },
  "peerDependencies": {
    "@hey-api/openapi-ts": "< 2",
    "nuxt": ">= 3.0.0 < 4",
    "vue": ">= 3.5.13 < 4"
  },
  "devDependencies": {
    "@config/vite-base": "workspace:*",
    "@hey-api/client-core": "workspace:*",
    "@hey-api/openapi-ts": "workspace:*",
    "@nuxt/test-utils": "3.17.2",
    "vite": "6.3.1",
    "vitest": "3.1.1"
  }
}
