{
  "name": "openapi-ts-monorepo",
  "version": "0.1.0",
  "description": "Public @hey-api monorepo",
  "private": true,
  "homepage": "https://heyapi.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hey-api/openapi-ts.git"
  },
  "bugs": {
    "url": "https://github.com/hey-api/openapi-ts/issues"
  },
  "license": "MIT",
  "author": {
    "email": "lubos@heyapi.dev",
    "name": "Hey API",
    "url": "https://heyapi.dev"
  },
  "funding": "https://github.com/sponsors/hey-api",
  "type": "module",
  "scripts": {
    "build": "turbo run build --filter=\"!@example/openapi-ts-sample\"",
    "changeset": "changeset",
    "examples:check": "sh ./scripts/examples-check.sh",
    "examples:generate": "sh ./scripts/examples-generate.sh",
    "format": "prettier --write .",
    "lint:fix": "prettier --check --write . && eslint . --fix",
    "lint": "prettier --check . && eslint .",
    "prepare": "husky",
    "test:changelog": "vitest run __tests__/*.test.ts",
    "test:changelog:watch": "vitest watch __tests__/*.test.ts",
    "test:coverage": "turbo run test:coverage",
    "test:update": "turbo run test:update",
    "test:watch": "turbo run test:watch",
    "test": "turbo run test --filter=\"!@example/openapi-ts-sample\"",
    "typecheck": "turbo run typecheck --filter=\"!@example/openapi-ts-sample\""
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "catalog:dev",
    "@changesets/cli": "catalog:dev",
    "@changesets/get-github-info": "catalog:dev",
    "@changesets/parse": "catalog:dev",
    "@changesets/types": "catalog:dev",
    "@config/vite-base": "workspace:*",
    "@eslint/js": "catalog:dev",
    "@hey-api/custom-client": "workspace:*",
    "@types/node": "catalog:dev",
    "@typescript-eslint/eslint-plugin": "catalog:dev",
    "@vitest/coverage-v8": "catalog:dev",
    "dotenv": "catalog:dev",
    "eslint": "catalog:dev",
    "eslint-config-prettier": "catalog:dev",
    "eslint-plugin-simple-import-sort": "catalog:dev",
    "eslint-plugin-sort-destructure-keys": "catalog:dev",
    "eslint-plugin-sort-keys-fix": "catalog:dev",
    "eslint-plugin-typescript-sort-keys": "catalog:dev",
    "eslint-plugin-vue": "catalog:dev",
    "globals": "catalog:dev",
    "husky": "catalog:dev",
    "lint-staged": "catalog:dev",
    "prettier": "catalog:dev",
    "rollup": "catalog:dev",
    "rollup-plugin-dts": "catalog:dev",
    "tsdown": "catalog:dev",
    "turbo": "catalog:dev",
    "typescript": "catalog:dev",
    "typescript-eslint": "catalog:dev",
    "vitest": "catalog:dev"
  },
  "packageManager": "pnpm@10.19.0"
}
